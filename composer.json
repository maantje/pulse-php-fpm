{
    "name": "maantje/pulse-php-fpm",
    "description": "A Laravel Pulse card to show php-fpm status",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Schouten",
            "email": "j4mie@hey.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "*",
        "laravel/pulse": "^1",
        "adoy/fastcgi-client": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8",
        "mockery/mockery": "^1.5.0",
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Maantje\\Pulse\\PhpFpm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maantje\\Pulse\\PhpFpm\\PhpFpmServiceProvider"
            ]
        }
    }
}
